{
    "name": "czim/laravel-cms-auth",
    "description": "Laravel CMS: Authentication Component",
    "authors": [
        {
            "name": "Coen Zimmerman",
            "email": "coen@pixelindustries.com"
        }
    ],
    "keywords": [ "cms", "laravel", "authentication" ],
    "homepage": "https://github.com/czim",
    "require": {
        "php": ">=7.1.3",
        "cartalyst/sentinel": "^2.0"
    },
    "require-dev": {
        "czim/laravel-cms-core": "~1.7",
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "~1.0",
        "orchestra/testbench": "~3.8",
        "orchestra/database": "~3.8",
        "satooshi/php-coveralls": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Czim\\CmsAuth\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Czim\\CmsAuth\\Test\\": "tests",
            "Czim\\CmsCore\\Test\\": "vendor/czim/laravel-cms-core/tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "suggest": {
        "czim/laravel-cms-auth-api": "Optional extension to support OAuth2 API authentication"
    }
}
